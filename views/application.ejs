        <style>
            a[href^="http://maps.google.com/maps"]{display:none !important}
            a[href^="https://maps.google.com/maps"]{display:none !important}

            .gmnoprint a, .gmnoprint span, .gm-style-cc {
                display:none;
            }
            .gmnoprint div {
                background:none !important;
            }
			.sidenav {
				height: 100%;
				width: 0;
				position: fixed;
				z-index: 1;
				top: 0;
				left: 0;
				background-color: black;
				overflow-x: hidden;
				transition: 0.5s;
				padding-top: 60px;
                opacity: .9;
                border-radius: 0px 50px 50px 0px

			}
			
			.sidenav a {
				padding: 8px 8px 8px 32px;
				text-decoration: none;
				font-size: 25px;
				color: #818181;
				display: block;
				transition: 0.3s;
                
			}
			
			.sidenav a:hover {
				color: #f1f1f1;
			}
			.sidenav .closebtn {
				position: absolute;
				top: 0;
				right: 25px;
				font-size: 36px;
				margin-left: 50px;
			}
			@media screen and (max-height: 450px) {
				.sidenav {padding-top: 15px;}
				.sidenav a {font-size: 18px;}
			}
            
            .wrapper {
                position: absolute;
                width: 100%;
                height: 100%;
            }


            .content {
            position: absolute;
            padding: 1px;
            top: 25px;
            left: 25px;
            background: white;
            opacity: 1;
            border-radius: 25px;
            }
            
            .content-right {
            position: absolute;
            padding: 1px;
            top: 25px;
            right: 25px;
            background: white;
            opacity: 1;
            border-radius: 25px;
            }
            .content-main{
            position: absolute;
            padding: 4px;
            top: 50px;
            left: 50%;
            margin-right: -50%;
            transform: translate(-50%, -50%);
            background: white;
            opacity: 1;
            border-radius: 25px;
            }
            .content-form{
            position: relative;
            }
            
            #map{
                width: 100%;
                height: 400px;
                border-radius: 25px;
            }
            input[type=text]{
            padding-top: 5px ;
            width: 20%;
            padding: 12px 20px;
            margin: 8px 0;
            box-sizing: border-box;
            border: 2px  	gray-black ;
            border-radius: 10px;
            }
            #addres_result_id{
                width: 500px;
                height: 200px;
            }

            .content-map{
                height: 100%;
                width: 100%;
            position: absolute;
            padding: 4px;
            top: 400px;
            left: 50%;
            margin-right: -50%;
            transform: translate(-50%, -50%);
            background: white;
            opacity: 1;
            }

            .content-search{
            position: absolute;
            padding-top: 20px;
            top: 120px;
            left: 50%;
            margin-right: -50%;
            transform: translate(-50%, -50%);
            background: clear;
            opacity: 1;
            border-radius: 0px;
            }
            .content-center{
                left: 50;
                margin-right: -50%;
            }

            .button5 {background-color: rgba(207, 184, 124, 1);
            width: 100%;}

            .textbox {border-radius:8px;
            width: 100%;}
            
            .invisibleDiv{
                opacity: 0;
            }
            
            .modal-backdrop {
                background-color: rgba(207, 184, 124, 1);
            }
