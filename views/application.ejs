    </script>

	<div id="mySidenav" class="sidenav">
		  <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
		  <a href="/home">Map</a>
		  <a href="/events">Events</a>
		  <a href="#">Search</a>
	</div>
    
    <div>
        <div class="content">
            <p class="my-2 col-2" style="font-size:30px;cursor:pointer" onclick="openNav()">&#9776;</p>
        </div>
      
        <div class="content-main"> <img src="cutherelogo"  alt="ll"> </div>
    </div>
    <br>
    <br>
    <br>
    <br>
<div class="container">
		<form method="POST" action="/application/done">
            

            <div id="map"></div>
            <br>
            <div class="form-group">
                <label for="input-article-title">Event Title</label>
                <input type="eventtitle" class="form-control" id="eventTitle"  placeholder="Title" name = "eventName">
              </div>
            <div class ="form-row">
                <div class="col">
                    <div class="form-group">
                            <label for="timeStart">Time Start</label>
                            <input type="time" class="form-control" id="timeStart" name = "timeStart">
                    </div> 
                </div>
            </div>
            
            <div class ="form-row">
                <div class="col">
                    <div class="form-group">
                            <label for="month">Time End</label>
                            <input type="time" class="form-control" id="timeEnd" name = "timeEnd">
                    </div> 
                </div>
            </div>
            
            <div class="form-row">
                <div class="col">
                    <label for="hourStart">Date</label>
                    <input type="date" class="form-control" id="date" name = "Date">
                </div>
                
            </div>
            <br>
            <div class='form-row'>
                <div class="col">
                    <label for="description">Event Description</label>
                    <textarea  id='des' rows="4" cols="157" name="description" class="textbox"></textarea>
                </div>
            </div>
            <br>
            <div class="form-group">
                    <button id="my_submit_button" class="btn button5" disabled>Apply</button>
           
            </div>
                <input type="text" name="Address" id="address_result_id" style="visibility: hidden"><br>
                <input type="text" name="lng" id="longitude_result_id" style="visibility: hidden"><br>
                <input type="text" name="lat" id="latitude_result_id" style="visibility: hidden"><br>
                <input type="text" name="orgID" id="org_id" style="visibility: hidden"><br>
        </form>
    </div>
    
    

    <div class="content-search">
            <form class="form-inline" action="javascript:void(0);">
                <input class="form-control dark" id="search_location" type="text" placeholder="Search location">
                <button class="btn btn-dark" id="search_button" type="button" onclick="findlat_alt();">Search</button>
            </form>
    </div> 

    <div class="modal fade" id="login" tabindex="-1" role="dialog" >
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" >Enter Key Code and Password</h5>
                </div>
                <div class="modal-body">
                                <input class="form-control dark" type="password" id="id_organizer" placeholder="Key Code">
                                <br>
                                <input class="form-control dark" type="password" id="password_organizer" placeholder="Password">
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" id='loginButton' onclick="verify()">Login</button>
                </div>
              </div>
            </div>
    </div>



